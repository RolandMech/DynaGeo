#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = DynaGeo.exe DynaGeo.ENG
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DynaGeo.exe: ..\DynaGeo.dpr
  $(DCC)

DynaGeo.ENG: DynaGeo.dpr
  $(DCC)


